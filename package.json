{
  "name": "dynamo-butter",
  "description": "A dynamo client enhancer",
  "author": "Tim Kye",
  "version": "2.0.0-6",
  "license": "Apache-2.0",
  "scripts": {
    "style": "prettier --write \"{src,test}/**/*.ts\"",
    "build:old": "rollup -c",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "check": "npm run style && npm run lint",
    "test": "npm run check && npm run test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test",
    "test:coverage": "jest && open coverage/index.html",
    "release": "npm run build && np"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/main.js",
      "require": "./dist/cjs/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Nike-Inc/dynamo-butter.git"
  },
  "engines": {
    "node": ">=12.9"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.4.1",
    "@aws-sdk/node-http-handler": "^3.5.0",
    "@aws-sdk/util-dynamodb": "^3.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nock": "^11.8.2",
    "np": "^6.5.0",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
